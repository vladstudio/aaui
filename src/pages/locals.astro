---
import Layout from "@layouts/Layout.astro"
const auth = Astro.locals.pb.authStore
---

<Layout title="Hi hi">
  <div x-init x-data="{}" class="grid w-full gap-4 p-4">
    auth:
    <pre class="whitespace-pre-wrap border p-4">{JSON.stringify(auth)}</pre>
    auth.isValid:
    <pre
      class="whitespace-pre-wrap border p-4">{JSON.stringify(auth.isValid)}</pre>
  </div>
</Layout>
